    "css-loader": "^6.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "faker": "^4.1.0",
    "html-loader": "^3.0.1",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "mini-css-extract-plugin": "^2.4.5",
